<li><%= link_to "Contacto", contact_path, class: 'btn' %></li>
<% if !user_signed_in? %>
<li><%= link_to "Regístrate", new_user_registration_path, class: 'btn' %></li>
<li><%= link_to "Iniciar Sesión", new_user_session_path, class: 'btn' %></li>
<% else %>
<li><%= link_to "Protected Page", inside_path, class: 'btn' %></li>
<li><%= link_to "Edit your account", edit_user_registration_path, class: 'btn' %></li>

<% if current_user.admin? %>
<li><%= link_to "Admin", admin_root_path, class: 'btn' %></li>
<% end %>

<li><%= link_to "Sign out", destroy_user_session_path, data:{method: :delete}, class: 'btn' %></li>
<% end %>
